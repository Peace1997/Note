{
  "name": "obsidian-prism",
  "version": "2.3.0",
  "description": "A Highly-Customisable & Elegant Light/Dark Theme for Obsidian.md",
  "author": "Damian Korcz",
  "license": "MIT",
  "homepage": "https://github.com/damiankorcz/Obsidian-Prism#readme",
  "scripts": {
    "dev": "node ./esbuild.config.mjs",
    "build": "node ./esbuild.config.mjs production"
  },
  "devDependencies": {
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.38",
    "esbuild-plugin-time": "^1.0.0",
    "esbuild-sass-plugin": "^2.2.6",
    "postcss": "^8.4.12",
    "stylelint": "^14.7.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-no-unsupported-browser-features": "^5.0.3"
  },
  "blue-topaz-theme@@transparent-setting-panel": false,
  "blue-topaz-theme@@color-scheme-options": "color-scheme-options-default",
  "blue-topaz-theme@@background-settings-workplace-background-image": false,
  "blue-topaz-theme@@simple-titlebar": false,
  "blue-topaz-theme@@background-image-settings-command-palette-switch": false,
  "blue-topaz-theme@@background-image-settings-command-palette-same-as-workplace": false,
  "blue-topaz-theme@@text-highlight-bg-h-light": 34,
  "things-style@@default-font-color": false,
  "things-style@@h4-color": "#E69818",
  "things-style@@h4-transform": "none",
  "things-style@@h1-color": "#0E69E8",
  "things-style@@h1-weight": 800,
  "things-style@@em-color": "#D25E5E",
  "things-style@@strong-color": "#E1729D",
  "things-style@@code-color-d": "#E2BCBC",
  "blue-topaz-theme@@line-height-main": 2,
  "blue-topaz-theme@@background-settings-workplace-theme-light": "background-settings-workplace-theme-light-blue-mountain",
  "blue-topaz-theme@@accent-strong@@dark": "#E67CCD",
  "things-style@@quote-color": "#A27DAA",
  "things-style@@code-normal": "#E89F9F",
  "things-style@@h1-size": "1.5em",
  "things-style@@h2-size": "1.3em",
  "things-style@@h4-size": "1.1em",
  "things-style@@h3-size": "1.2em"
}